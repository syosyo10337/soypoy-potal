{
  "name": "soypoy-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenvx run -f .env.local -- next dev",
    "build": "pnpm db:migrate && NODE_ENV=production next build",
    "start": "next start",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "tsc": "tsc --noEmit",
    "check": "concurrently --kill-others-on-fail \"pnpm tsc\" \"pnpm lint:fix\" && pnpm format",
    "db:migrate": "tsx scripts/migrate.ts",
    "drzl:gen": "dotenvx run -f .env.local --overload -- drizzle-kit generate",
    "drzl:migrate": "dotenvx run -f .env.local --overload -- drizzle-kit migrate",
    "drzl:studio": "dotenvx run -f .env.local --overload -- drizzle-kit studio"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.51.1",
    "@hookform/resolvers": "^5.2.2",
    "@neondatabase/serverless": "^1.0.2",
