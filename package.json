  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.5.13",
    "concurrently": "^9.1.0",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.6",
    "svgo": "^4.0.0",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5",
    "ws": "^8.18.0"
